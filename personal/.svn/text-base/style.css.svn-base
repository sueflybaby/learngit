.per_table{
	width:250px;
	height: 300px;
	padding:10px;
	text-align:center;
	font-family:18px;
	font-weight:bolder;
	border: 0px;
	margin-left: -125px;
}

.per_jifen{
	width:100%;
	text-align:left;
	font-family:18px;
	margin-left:-50%;
	margin-top:30%;
}
a:link {color: blue; text-decoration:none;} //未访问：蓝色、无下划线
img{
	width:40px;
	height:40px;
	}
#headpart{
	width: 100%;
	height:140px;
}
#pic{
	position: absolute;
	left: 50%;
	margin-left: -60px;
	margin-top: 10px;
	width:120px;height:120px;border-radius: 10px;
	background-image: url(index.jpg);
	background-repeat: no-repeat;
}
#admin_jifen{
	position: absolute;
	left: 50%;
	margin-top: 120px;
	margin-left: -100px;
	height: 20px;
	width: 200px;
	text-align: center;
	font-size: 14px;
	font-weight: bold;
}

#shopcenter{
	width: 100%;
	padding: 25px 0px 0px 15px;
	display: none;
	
}
#shopcenter_title{
	width: 100px;
	height:50px;
	font-weight: bold;
	margin: 0px;
	float: left;
	text-align: justify;
	font-size: 20px;
	padding-top: 40px;
	padding-bottom: 0px;

}
#shopcenter_content{
	position:absolute;

	margin-left: 50px;

	padding-top: 10px;
	text-align: justify;

}

#history{
	width: 100%;
	height: 120px;

	padding: 15px 0px 0px 15px;
	display: none;
}
#history_title{
	width: 150px;
	height:80px;
	font-weight: bold;
	margin: 0px;
	float: left;
	text-align: justify;
	font-size: 20px;
	padding-top: 40px;
	padding-bottom: 0px;

}
#history_content{
	position:absolute;
	margin-left: 40px;
	padding: 10px;
	font-size: 8px;
}

#endpart{
	width:100%;
	position: fixed;
	bottom: 0;
	line-height:20px;
	color:#bababa;
	text-align:center;
	height: 25px;
	font-size:14px;
	border-top:1px dashed #CCC;

	background: red;
}
.seperate{
	clear: both;

}
.lanmu{
	width: 100%;
	height: 300px;
	text-align: center;
	margin: 10px;
	
}
